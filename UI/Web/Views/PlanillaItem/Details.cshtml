@model SistemaMAV.UI.Web.Models.PlanillaItemViewModel

@{
    ViewData["Title"] = "Detalles del Item de Planilla de Mantenimiento " + Model.Planilla.Detalle + " - " + Model.ItemMantenimiento.Detalle;
    string objName = this.ViewContext.RouteData.Values["controller"].ToString();
}

<div class="row">
    <div class="col-xs-12">
        <div class="box box-primary">
            <div class="box-header">
                <h3 class="box-title">@ViewBag.Title</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <dl class="row">
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.Planilla)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.Planilla.Detalle)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.ItemMantenimiento)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.ItemMantenimiento.Detalle)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.Kilometros)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.Kilometros)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.Meses)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.Meses)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.Recomendaciones)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.Recomendaciones)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.Observaciones)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.Observaciones)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.InfoExtra)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.InfoExtra)
                    </dd>
                </dl>
                <p>
                    <a class="btn btn-primary" href="/@Html.Raw(objName)/Edit/@Html.Raw(Model.PlanillaId)" title="Modificar"><span class="glyphicon glyphicon-pencil" style="margin-right: 5px;"></span>Modificar</a>
                    <a class="btn btn-default" href="/@Html.Raw(objName)/Index" title="Volver"><span class="glyphicon glyphicon-arrow-left" style="margin-right: 5px;"></span>Volver</a>
                </p>
            </div>
        </div>
    </div>
</div>
